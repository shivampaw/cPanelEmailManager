<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<DialogPane xmlns:fx="http://javafx.com/fxml" fx:controller="com.shivampaw.cpanelemailmanager.controller.NewMailboxesController">
    <content>
        <GridPane vgap="10" hgap="10">
            <Label text="Username" GridPane.columnIndex="0" GridPane.rowIndex="0" />
            <TextField fx:id="mailboxUsername" GridPane.columnIndex="1" GridPane.rowIndex="0" />

            <Label text="Password" GridPane.columnIndex="0" GridPane.rowIndex="1" />
            <PasswordField fx:id="mailboxPassword" GridPane.columnIndex="1" GridPane.rowIndex="1" />

            <Label text="Quota (MB) (0 for unlimited)" GridPane.columnIndex="0" GridPane.rowIndex="2" />
            <TextField fx:id="mailboxQuota" GridPane.columnIndex="1" GridPane.rowIndex="2" />
        </GridPane>
    </content>
</DialogPane>
