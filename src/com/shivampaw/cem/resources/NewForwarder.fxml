<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<DialogPane xmlns:fx="http://javafx.com/fxml" fx:controller="com.shivampaw.cem.java.controller.NewForwarderController">
    <content>
        <GridPane vgap="10" hgap="10">
            <Label text="Forward From" GridPane.columnIndex="0" GridPane.rowIndex="0" />
            <TextField fx:id="forwardFrom" GridPane.columnIndex="1" GridPane.rowIndex="0" />

            <Label text="Forward To" GridPane.columnIndex="0" GridPane.rowIndex="1" />
            <TextField fx:id="forwardTo" GridPane.columnIndex="1" GridPane.rowIndex="1" />
        </GridPane>
    </content>
</DialogPane>
